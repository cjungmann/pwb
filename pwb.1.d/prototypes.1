.\" -*- mode: nroff -*-
.de pt_help
.  B pwb help
.  sp 0
..
.de pt_init
.  B pwb init
.  sp 0
..
.de pt_restore
.  B pwb restore
.  sp 0
..
.de pt_declare
.  B pwb declare
.  cli_prototype @new_handle_name @data_source_name @data_count @printer_func_name \
           ?!-h \
           ?!-e:exec_func_name ?!-d:extra_data_name \
           ?!-b:bottom_margin_print_function ?!-l:left_margin_print_function \
           ?!-r:right_margin_print_function ?!-t:top_margin_print_function
.  sp 0
..
.de pt_get_dimensions
.  B pwb get_dimensions
.  cli_prototype @handle @associative_array_name
.  sp 0
..
.de pt_measure_string
.  B pwb measure_string
.  cli_prototype ?!-v:variable_name @string
.  sp 0
..
.de pt_limit_print
.  B pwb limit_print
.  cli_prototype @length @string
..
.de pt_get_keystroke
.  B pwb get_keystroke
.  cli_prototype ?!-v:variable_name
..
.de pt_erase_head
.  B pwb erase_head
.  cli_prototype @handle
.  sp 0
..
.de pt_declare_keymap
.  B pwb declare_keymap
.  cli_prototype @new_keymap_name @array_name
.  sp 0
..
.de pt_start
.  B pwb start
.  cli_prototype @handle ?!-t:top_row ?!-f:focus_row ?!-b:base_keymap ?!-a:auxiliary_keymap
.sp 0
..
.de pt_set_margins
.  B pwb set_margins
.  cli_prototype @handle @top ?@right ?@bottom ?@left
.sp 0
..
.de pt_print_row
.  B pwb print_row
.  cli_prototype @handle @line_number
.sp 0
..
.de pt_print_all
.  B pwb print_all
.  cli_prototype @handle
.sp 0
..
.de pt_print_callback
.  B print_function
.  cli_prototype @data_row_number @data_source_name @chars_to_print @focus_flag @pwb_handle_name ?@extra_data_name
..
.de pt_exec_callback
.  B exec_function
.  cli_prototype @keystroke @data_row_number @data_source_name @pwb_handle_name ?@extra_data_name
..
